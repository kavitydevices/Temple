<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid android:color="@color/cl"/>
        </shape>
    </item>

    <item android:top="15dp"
        android:bottom="15dp"
        android:right="15dp"
        android:left="15dp">
        <bitmap android:src="@drawable/mob"/>
    </item>
    <item android:top="5dp"
        android:bottom="5dp"
        android:right="5dp"
        android:left="5dp">
        <shape android:shape="oval">
            <stroke android:color="@color/whiteCL" android:width="2dp"/>
        </shape>
    </item>

</layer-list>